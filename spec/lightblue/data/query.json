{
    "objectType":"country",
    "projection": { "field":"*","recursive":1},
    "sort" : { "iso3Code":"$asc"},
    "query":  { "$all" : [
        { "$and": [{ "field":"iso2Code", "op":"$eq", "rvalue":"US" },
                   { "field":"iso3Code", "op":"$eq", "rvalue":"USA"}
                  ]
        },
        { "$not" : { "field":"iso2Code", "op":"$eq", "rvalue":"US" } }

    ] },
    "range" : [0,10]
}
